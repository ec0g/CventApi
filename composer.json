{
  "name": "CventApi",
  "description": "CventApi is a library package used to interact with Cvent's SOAP Api",
  "license": "MIT",
  "keywords": [
    "cvent,soap"
  ],
  "authors": [
    {
      "name": "Goce Evtimovski",
      "email": "goce@lmo.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "vlucas/phpdotenv": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "CventApi\\": "src/"
    }
  }
}
